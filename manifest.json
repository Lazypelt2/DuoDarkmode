	"description": "Dark mode for Duolingo",

	"content_scripts": [{
		"css": ["styles.css"],
		"js": ["content.js"],
		"matches": ["https://www.duolingo.com/*","https://www-internal.duolingo.com/*", "https://join.duolingo.com/*", "https://preview.duolingo.com/*"]
	}]

}